<div id="tweet_<%= tweet.id %>" class="query_tweets_row">
	<div class="cell text"><%= auto_link( tweet.text, nil, { :target => "_blank" } ) %></div>
	<div class="cell user"><%= tweet.from_user %></div>
	<div class="cell location"><%= Twitter.user(tweet.from_user).location %></div>
	<div class="cell date"><%= tweet.created_at.to_date %></div>
</div>